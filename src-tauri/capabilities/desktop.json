{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "autostart:default",
    "process:default",
    "process:allow-restart",
    "process:allow-exit",
    "updater:default",
    "core:window:allow-set-always-on-top",
    "core:window:allow-unminimize"
  ]
}